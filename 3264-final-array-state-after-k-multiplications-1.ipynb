   "source": [
    "from typing import List\n",
    "\n",
    "class Solution:\n",
    "    def getFinalState(self, nums: List[int], k: int, multiplier: int) -> List[int]:\n",
    "        while k > 0:\n",
    "            idx = nums.index(min(nums))\n",
    "            nums[idx] *= multiplier\n",
    "            k -= 1\n",
    "        return nums"
   ]
